<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
    <declaration>// Place global declarations here.</declaration>
    <template>
        <name x="5" y="5">Template</name>
        <declaration>// Place local declarations here.
            clock x,y;</declaration>
        <location id="id0" x="-212" y="-102">
            <label kind="invariant" x="-222" y="-85">x&lt;=2</label>
        </location>
        <location id="id1" x="-119" y="-136">
            <label kind="invariant" x="-129" y="-119">y&lt;=10</label>
        </location>
        <location id="id2" x="-119" y="-68">
        </location>
        <location id="id3" x="-25" y="-187">
            <name x="-35" y="-221">Goal</name>
        </location>
        <location id="id4" x="-17" y="-119">
        </location>
        <init ref="id0"/>
        <transition>
            <source ref="id0"/>
            <target ref="id0"/>
            <label kind="guard" x="-297" y="-153">x&lt;1</label>
            <label kind="assignment" x="-289" y="-93">x=0</label>
            <nail x="-263" y="-153"/>
            <nail x="-263" y="-76"/>
        </transition>
        <transition>
            <source ref="id1"/>
            <target ref="id3"/>
            <label kind="guard" x="-85" y="-187">x&gt;4</label>
        </transition>
        <transition>
            <source ref="id1"/>
            <target ref="id4"/>
            <label kind="guard" x="-76" y="-153">y&lt;4</label>
        </transition>
        <transition>
            <source ref="id0"/>
            <target ref="id2"/>
            <label kind="guard" x="-170" y="-76">y&lt;2</label>
        </transition>
        <transition>
            <source ref="id0"/>
            <target ref="id1"/>
            <label kind="assignment" x="-170" y="-144">y=0</label>
        </transition>
    </template>
    <system>// Place template instantiations here.
        Process = Template();
        // List one or more processes to be composed into a system.
        system Process;
    </system>
    <queries>
        <query>
            <formula>Pr[&lt;=100](&lt;&gt;Process.Goal)</formula>
            <comment></comment>
        </query>
    </queries>
</nta>