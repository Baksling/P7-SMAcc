<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.
			clock x;</declaration>
		<location id="id0" x="-110" y="-110">
			<label kind="invariant" x="-135" y="-102">x &lt;= 10</label>
		</location>
		<location id="id1" x="-8" y="-152">
		</location>
		<location id="id2" x="-8" y="-68">
			<name x="9" y="-76">Goal</name>
			<label kind="exponentialrate" x="-17" y="-51">1</label>
		</location>
		<location id="id3" x="-25" y="-246">
		</location>
		<location id="id4" x="0" y="-204">
		</location>
		<branchpoint id="id5" x="-68" y="-178">
		</branchpoint>
		<init ref="id0"/>
		<transition>
			<source ref="id5"/>
			<target ref="id4"/>
			<label kind="probability" x="-42" y="-187">2</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id3"/>
			<label kind="probability" x="-59" y="-229">3</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id5"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="guard" x="-76" y="-85">x &lt;=3</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
		</transition>
	</template>
	<system>// Place template instantiations here.
		Process = Template();
		// List one or more processes to be composed into a system.
		system Process;
	</system>
	<queries>
		<query>
			<formula>Pr[&lt;=100](&lt;&gt;Process.Goal)</formula>
			<comment></comment>
		</query>
	</queries>
</nta>