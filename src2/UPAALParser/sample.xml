<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.
clock x;</declaration>
		<location id="id0" x="-144" y="-144">
		</location>
		<location id="id1" x="-186" y="-59">
			<label kind="invariant" x="-95" y="76">x &gt; 5</label>
		</location>
		<location id="id2" x="-102" y="-59">
			<label kind="invariant" x="-95" y="76">x &gt; 5</label>
		</location>
		<location id="id3" x="-187" y="59">
			<label kind="invariant" x="-95" y="76">x &gt; 5</label>
		</location>
		<location id="id4" x="-85" y="59">
			<label kind="invariant" x="-95" y="76">x &gt; 5</label>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id4"/>
			<target ref="id3"/>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id4"/>
			<label kind="guard" x="-93" y="-34">x &lt; 5</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id3"/>
			<label kind="guard" x="-186" y="-34">x &lt; 10</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="guard" x="-119" y="-127">x &lt;= 3</label>
			<label kind="assignment" x="-110" y="-110">x = 5</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="guard" x="-212" y="-127">x &lt;= 10</label>
			<label kind="assignment" x="-212" y="-110">x = 0</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
