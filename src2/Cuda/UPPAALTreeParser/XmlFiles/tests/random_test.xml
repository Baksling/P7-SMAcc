<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
double x, y = 0.0;</declaration>
	<template>
		<name x="5" y="5">random_test</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="0" y="0">
			<label kind="exponentialrate" x="-10" y="34">1</label>
		</location>
		<location id="id1" x="110" y="0">
			<label kind="exponentialrate" x="100" y="34">1</label>
		</location>
		<location id="id2" x="204" y="-68">
			<name x="194" y="-102">Goal</name>
		</location>
		<location id="id3" x="204" y="68">
		</location>
		<branchpoint id="id4" x="204" y="0">
		</branchpoint>
		<init ref="id0"/>
		<transition>
			<source ref="id4"/>
			<target ref="id3"/>
			<label kind="probability" x="212" y="17">x</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id2"/>
			<label kind="probability" x="212" y="-34">y</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id4"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="assignment" x="8" y="17">x = random(90),
y = random(10)</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = random_test();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
		<query>
			<formula>Pr[&lt;=100;100000](&lt;&gt; Process.Goal)</formula>
			<comment></comment>
		</query>
	</queries>
</nta>
