<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here. 
clock x,y;</declaration>
		<location id="id0" x="0" y="0">
			<label kind="invariant" x="-10" y="17">x&lt;=10</label>
		</location>
		<location id="id1" x="-34" y="-85">
			<name x="-44" y="-119">Goal</name>
		</location>
		<location id="id2" x="42" y="-85">
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="guard" x="25" y="-51">x&gt;=0</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="guard" x="-17" y="-85">y&lt;=10</label>
			<nail x="0" y="-51"/>
		</transition>
	</template>
	<system>// Place template instantiations here.
		Process = Template();
		// List one or more processes to be composed into a system.
		system Process;
	</system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
