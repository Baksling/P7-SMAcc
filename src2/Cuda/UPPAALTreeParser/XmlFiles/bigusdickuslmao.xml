<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
    <declaration>// Place global declarations here.</declaration>
    <template>
        <name x="5" y="5">Template</name>
        <declaration>// Place local declarations here.
clock x;</declaration>
        <location id="id0" x="0" y="0">
            <label kind="exponentialrate" x="-10" y="34">0.3</label>
        </location>
        <location id="id1" x="127" y="-59">
            <name x="117" y="-93">Goal</name>
        </location>
        <location id="id2" x="119" y="34">
        </location>
        <init ref="id0"/>
        <transition>
            <source ref="id0"/>
            <target ref="id2"/>
            <label kind="guard" x="18" y="-17">x&lt;8</label>
        </transition>
        <transition>
            <source ref="id0"/>
            <target ref="id1"/>
            <label kind="guard" x="18" y="-63">x&gt;=8</label>
        </transition>
    </template>
    <system>// Place template instantiations here.
        Process = Template();
        // List one or more processes to be composed into a system.
        system Process;
    </system>
    <queries>
        <query>
            <formula></formula>
            <comment></comment>
        </query>
    </queries>
</nta>